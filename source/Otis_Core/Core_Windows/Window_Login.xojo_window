#tag Events PushButton_Ok
	#tag Event
		Sub Action()
		  dim saveUN as Boolean
		  dim savePass as Boolean
		  
		  
		  // Open our progress window
		  
		  saveUN = CheckBox_saveUsername.Value
		  savePass = CheckBox_savePassword.Value
		  
		  
		  
		  // Update our login information from TextFields
		  dbUsername = TextField_Username.Text.ToText
		  dbPassword = TextField_Password.Text.ToText
		  dbHost = TextField_serverAddress.Text.ToText
		  dbPort = val( TextField_Port.Text )
		  dbName = TextField_Database.Text.ToText
		  
		  
		  // Save the Username is instructed to
		  If saveUN = True then
		    saveUserName
		  End If
		  
		  // Save the password if instructed to 
		  If savePass = True Then
		    savePassword
		  End If 
		  
		  // Lets Login
		  If dbUsername <> "" And dbPassword <> "" And dbHost <> "" And dbPort <> 0 And dbName <> "" Then
		    If login Then  'Login Successful
		      me.Window.Close
		      MainWindow.myOpen
		    Else
		      logErrorMessage( 4, "DBase", "Could Not Log In" )
		    End If
		  End If
		  
		  
		  
		End Sub
	#tag EndEvent
